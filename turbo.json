{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**", "public/**", "index.html", "vite.config.*", "package.json"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "dev:backend": {
      "cache": false,
      "persistent": true
    }
  }
}
